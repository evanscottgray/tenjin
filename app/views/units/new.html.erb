<h4>New Unit</h4>
<%=form_for [@course, @resource] do |f|%>
<div class="form-group">
  <%=f.label :name%>
  <%=f.text_field :name, class: "form-control md"%>
</div>
<div class="form-group">
  <%=f.label :type%>
  <%=f.text_field :type, class: "form-control md"%>
</div>
<%=f.submit "Submit", class: "btn btn-default"%>
<%=link_to 'Back', courses_resources_path(@course), class: "btn btn-default"%>
<%end%>
